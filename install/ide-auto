# vim: set ft=sh:

install ()
{
    MODULES=""
    for m in $(auto_modules "/ide/"); do
        MODULES="$MODULES $(basename $m)"
    done
    BINARIES=""
    FILES=""
    SCRIPT="ide"
}

help ()
{
cat<<HELPEOF
  This hook loads the necessary modules for an ide root device.
  Detection will take place at runtime.
HELPEOF
}
